  "bugs": {
    "url": "https://github.com/CSC4990-Project-Serve/Restaurant-Reservation-Tracker/issues"
  },
  "homepage": "https://github.com/CSC4990-Project-Serve/Restaurant-Reservation-Tracker#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "concurrently": "^7.0.0",
    "serve": "^13.0.2"
  }
}
